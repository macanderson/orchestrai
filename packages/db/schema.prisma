// Prisma schema for Supabase (PostgreSQL)

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Example model
model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  name  String
  // Add more fields as needed
} 